{
  "projects": {
    "client": {
      "src": "client",
      "use": "@vercel/static-build",
      "buildCommand": "npm install && npm run build",
      "outputDirectory": "build"
    },
    "server": {
      "src": "server",
      "use": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
